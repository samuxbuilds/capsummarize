{
  "env": {
    "browser": true,
    "es2022": true,
    "webextensions": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off"
  },
  "ignorePatterns": ["*.ts"]
}
}
